{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchintu/react-boilerplate.git"
  },
  "keywords": [
    "[",
    "\"Node\"",
    "\"React\"",
    "]"
  ],
  "author": "Chinmaya Polukonda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cchintu/react-boilerplate/issues"
  },
  "homepage": "https://github.com/cchintu/react-boilerplate#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.5.0"
  }
}
